{
  "version": 2,
  "builds": [
    {
      "src": "api/[...].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/[...].js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}

